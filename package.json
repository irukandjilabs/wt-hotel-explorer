{
  "name": "wt-hotel-explorer",
  "version": "0.4.2",
  "description": "Web app to show hotel inventory from WT api.",
  "scripts": {
    "lint": "eslint src test --ext jsx,js",
    "test-watch": "jest --watch",
    "test": "rimraf coverage && jest --coverage",
    "coverage": "cat coverage/lcov.info | coveralls",
    "start": "NODE_ENV=development GIT_REV=`git rev-parse --short HEAD` WT_READ_API=https://playground-api.windingtree.com WT_SEARCH_API=https://playground-search-api.windingtree.com ./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --disable-host-check",
    "update-country-list": "node ./management/get-country-codes.js > src/assets/country-list.json",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --mode=production",
    "docker-start": "./node_modules/.bin/superstatic --debug 1 --port 8080 --host 0.0.0.0 --gzip 1 public/",
    "deploy-aws-playground": "./management/deploy-aws.sh playground",
    "deploy-aws-demo": "./management/deploy-aws.sh demo",
    "build-for-swarm-playground": "NODE_ENV=production GIT_REV=$TRAVIS_COMMIT HASH_ROUTED_BUILD=1 WT_READ_API='https://playground-api.windingtree.com' WT_SEARCH_API=https://playground-search-api.windingtree.com ./node_modules/.bin/webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/windingtree/wt-hotel-explorer"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@windingtree/wt-ui": "^1.1.1",
    "animate.css": "^3.7.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^5.0.1",
    "currency.js": "^1.2.1",
    "dayjs": "^1.7.7",
    "formik": "^1.3.1",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.3",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "popper.js": "^1.14.5",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-animate-on-scroll": "^2.1.5",
    "react-bootstrap-typeahead": "^3.2.4",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-leaflet": "^2.1.2",
    "react-loadable": "^5.4.0",
    "react-markdown": "^4.0.3",
    "react-markdown-renderer": "^1.4.0",
    "react-masonry-css": "^1.0.11",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "redux-thunk-actions": "^1.2.1",
    "reselect": "^4.0.0",
    "websocket": "^1.0.28",
    "windingtree-media-web": "git+https://github.com/windingtree/media.git#web-assets"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.2",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.5",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.1",
    "superstatic": "^6.0.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "testRegex": "./test/.*.spec.jsx",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.jsx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
